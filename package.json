{
  "name": "nestjs-extra",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "keywords": [
    "nestjs"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --cache --ext .js,.ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "jaeger-client": "3.17.2",
    "nats": "1.4.8",
    "nats-hemera": "7.0.2",
    "opentracing": "0.14.4",
    "pg": "8.0.2",
    "pino": "6.2.0",
    "pino-pretty": "4.0.0",
    "pretty-hrtime": "1.0.3",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "sql-formatter": "2.3.3"
  },
  "peerDependencies": {
    "@nestjs/common": "7.0.8",
    "@nestjs/core": "7.0.8",
    "@nestjs/microservices": "7.0.8",
    "knex": "0.21.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "7.0.8",
    "@nestjs/common": "7.0.8",
    "@nestjs/core": "7.0.8",
    "@nestjs/microservices": "7.0.8",
    "@types/node": "10.17.20",
    "@types/pg": "7.14.3",
    "@types/pino": "6.0.0",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-jest": "23.8.2",
    "knex": "0.21.0",
    "jest": "25.4.0",
    "prettier": "2.0.4",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
